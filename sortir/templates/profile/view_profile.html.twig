{% extends 'base.html.twig' %}
{% block css %}
    <link rel="stylesheet" href="{{ asset('css/profil.css') }}">
{% endblock %}
{% block main %}

     <h1>{{ profil.username }}</h1>


     <img src="{{ asset("img/profile_pic/") }}{{ profil.id }}.png"
          onerror="this.src='data:image/jpeg;base64,';"
          width="200" height="100">


     <div class="col-md-8 blabla">
         <div class="card mb-3">
             <div class="card-body">
                 <div class="row">
                     <div class="col-md-3">
                         <h6 class="mb-0">Prénom:</h6>
                     </div>
                     <div class="col-md-9 text-secondary text-center">
                         {{ profil.prenom }}
                     </div>
                 </div>
                 <hr>
                 <div class="row">
                     <div class="col-md-3">
                         <h6 class="mb-0">Nom:</h6>
                     </div>
                     <div class="col-md-9 text-secondary text-center">
                         {{ profil.nom }}
                     </div>
                 </div>
                 <hr>
                 <div class="row">
                     <div class="col-md-3">
                         <h6 class="mb-0">Téléphone:</h6>
                     </div>
                     <div class="col-md-9 text-secondary text-center">
                         {{ profil.telephone }}
                     </div>
                 </div>
                 <hr>
                 <div class="row">
                     <div class="col-md-3">
                         <h6 class="mb-0">Email:</h6>
                     </div>
                     <div class="col-md-9 text-secondary text-center">
                         {{ profil.mail }}
                     </div>
                 </div>
                 <hr>
                 <div class="row">
                     <div class="col-md-3">
                         <h6 class="mb-0">Site de rattachement:</h6>
                     </div>
                     <div class="col-md-9 text-secondary text-center">
                         {{ profil.site.nom }}
                     </div>
                 </div>
             </div>
         </div>
          <table class="table table-responsive" style="width:100%">
            <thead>
            <tr>
                <th>Nom du groupe</th>
                <th> Membres</th>
                <th>Action
            </tr>
            </thead>
            <tbody>
            {% for group in groups %}
                <tr>
                    <td>{{ group.nom }}</td>
                    <td><ul>{% for participant in group.utilisateurs %}
                            <li><a href="{{ path('profile-affichage-autre', {id:participant.id}) }}"> {{participant.nom}} {{participant.prenom}}</li>
                            {% endfor %}
                        </ul>
                    </td>
                    <td>{% if group.createur.id == profil.id %}
                        <a href="{{ path('group-edit', {id:group.id}) }}">Modifier</a>
                        <a href="{{ path('group-delete', {id:group.id}) }}">Supprimer</a>
                        {% endif %}
                    </td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
         <div class="row justify-content-center ">

             <a href="{{ path('profile-modified') }}" class="btn btn-primary btn-d-lg-flex mr-3 ">
                 Modifier mon profil</a>
             <a href="{{ path('home') }}" class="btn btn-primary btn-d-lg-flex btn btn-primary btn-d-lg-flex mr-3">
                 Retour</a>
             <a class="btn btn-primary" href="{{ path('my-sortie') }}">
                 Mes sorties</a>
         </div>
     </div>

 {% endblock %}

